{
  "name": "media",
  "version": "1.0.0",
  "description": "realme project, media services",
  "main": "build/server.js",
  "scripts": {
    "dev": "nodemon --watch \"*.ts\" --exec \"ts-node\" --project ./tsconfig.json ./src/server.ts",
    "build": "yarn tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node ./build/media/src/server.js",
    "docker-build": "docker build -t realme-media-services .",
    "docker-run": "docker run -p 3000:3000 realme-media-services"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "nodemon": "^3.1.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.3.0"
  }
}
